{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/pages/evento/Asistentes.tsx",
    "src/pages/evento/Balance.tsx",
    "src/pages/evento/Gastos.tsx",
    "src/pages/evento/Precios.tsx",
    "src/pages/evento/Proveedores.tsx",
    "src/pages/evento/Ubicaciones.tsx",
    "src/pages/evento/Resumen.tsx"
  ]
}